.rechargeListDetail {
  width: 100%;
  height: 100vh;
  background: #eee;
  padding: 20rpx 20rpx;
}

.detail {
  width: 100%;
  height: calc(100vh - 40rpx);
  background: #fff;
  border-radius: 10rpx;
  padding: 10rpx;
  font-size: 28rpx;
}

.detail-money {
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.detail-type {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.title {
  color: #888;
}

.money {
  font-size: 44rpx;
  font-weight: 600;
}
