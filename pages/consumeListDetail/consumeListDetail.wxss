.container_consume{
  width: 100%;
  height: 100vh;
  background: #eee;
  padding: 20rpx;
}
.consume{
  width: 100%;
  height: 100%;
  background: #fff;
  border-radius: 10rpx;
  padding: 10rpx;
  font-size: 28rpx;
}
.consume view{
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 50rpx;
}
.consume view .left{
  color: #888;
}
.consume view .right{
  color: #000;
}
.line{
  width: 100%;
  height: 2rpx;
  background: #ccc;
  margin: 10rpx 0;
}
